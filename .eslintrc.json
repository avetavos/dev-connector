{
  "env": {
    "browser": true,
    "commonjs": true
  },
  "extends": ["airbnb", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "no-console": "off",
    "import/no-extraneous-dependencies": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off"
  }
}
